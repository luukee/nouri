    font-weight: $font-weight-bold;
    @include transform(translateY(-50%));

    @include media-query($small) {
        @include transform(translate(-50%, 0));
        margin-top: $gutter;
        text-align: center;
    }
}

.map-section__content-wrapper {
    position: relative;
    text-align: center;
    height: 100%;
}

.map-section__content {
    position: relative;
    display: inline-block;
    background-color: $color-main-bg;
    padding: $gutter * 2;
    margin-top: $gutter;
    width: 100%;
    text-align: center;
    z-index: $z-index-base;

    @include media-query($medium-up) {
        position: absolute;
        left: 0;
        top: 50%;
        @include transform(translateY(-50%));
        margin-top: 0;
        width: percentage(1 / 3);
    }
}

.map-section__link {
    display: block;
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    z-index: $z-index-base - 1;
}

// Optically center map in visible area with
// extended height/widths and negative margins
.map-section__container {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 150%;
    margin-bottom: -50%;

    @include media-query($medium-up) {
        width: 130%;
        height: 100%;
        margin: 0 -30% 0 0;
    }
}

.map-section__button {
    display: inline-block;
    margin-top: $gutter;
    white-space: normal;
}

// Hide Google maps UI
.gm-style-cc,
.gm-style-cc + div {
    visibility: hidden;
}

.highlight {
    padding: ($section-mobile-space + ($gutter * 2)) 0 $section-mobile-space;

    @include media-query($medium-up) {
        padding: $section-desktop-space 0;
    }
}

.highlight__wrapper {
    position: relative;
    padding: ($gutter * 3) ($gutter * 2);

    @include media-query($medium-up) {
        @include display-flexbox();
    }
}

.highlight__content-container,
.highlight__image-container {
    @include media-query($medium-up) {
        @include flex(1);
        width: 50%;

        .highlight--align-top & {
            @include align-self(flex-start);
        }

        .highlight--align-middle & {
            @include align-self(center);
        }

        .highlight--align-bottom & {
            @include align-self(flex-end);
        }
    }
}

.highlight__content-container {
    @include media-query($medium-up) {
        @include flex-order(2);
    }

    @include media-query($small) {
        width: 100%;
        margin-top: $gutter * 3;
    }
}

.highlight__border {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    pointer-events: none;
    z-index: $z-index-base;

    &.has-animated,
    .supports-no-js &,
    .animations--disabled & {
        .highlight__line--top,
        .highlight__line--bottom {
            transition: width 0.5s ease-out 0.75s;
            width: 100%;
        }

        .highlight__line--right,
        .highlight__line--left {
            transition: height 0.5s ease-out 0.75s;
            height: 100%;
        }
    }
}

.highlight__line {
    position: absolute;
}

.highlight__line--top {
    top: 0;
    left: 0;
    height: 3px;
    width: 0;
}

.highlight__line--right {
    top: 0;
    right: 0;
    height: 0;
    width: 3px;
}

.highlight__line--bottom {
    bottom: 0;
    right: 0;
    height: 3px;
    width: 0;
}

.highlight__line--left {
    bottom: 0;
    left: 0;
    height: 0;
    width: 3px;
}

.highlight__image-container {
    position: relative;

    @include media-query($medium-up) {
        @include flex-order(1);
    }

    @include media-query($small) {
        position: relative;
        width: 100%;
        margin-top: -($gutter * 5);
        z-index: $z-index-base + 1;
    }
}

.highlight__image-container--front {
    @include media-query($medium-up) {
        z-index: $z-index-base + 2;
    }
}

.highlight__image-container--back {
    @include media-query($medium-up) {
        z-index: 0;
    }
}

.highlight__image {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;

    @include media-query($small) {
        display: block;
        margin: 0 auto;
    }

    @include media-query($medium-up) {
        margin-left: -25%;
    }
}

.highlight__image--placeholder {
    position: relative;
}

$slow-drawer-transition: transform 0.45s cubic-bezier(0.645, 0.045, 0.355, 1);
$fast-drawer-transition: transform 0.55s cubic-bezier(0.645, 0.045, 0.355, 1);

.drawer {
    position: fixed;
    top: 0;
    right: -$cart-mobile-width;
    width: $cart-mobile-width;
    height: 100vh;
    transition: $fast-drawer-transition;
    @include transform(translateX(0));
    z-index: $z-index-high;

    @include media-query($medium-up) {
        right: -$cart-desktop-width;
        width: $cart-desktop-width;
    }

    .drawer--active & {
        display: block;
        transition: $slow-drawer-transition;
        @include transform(translateX(-$cart-mobile-width));

        @include media-query($medium-up) {
            @include transform(translateX(-$cart-desktop-width));
        }
    }
}

[data-drawer-push] {
    transition: $slow-drawer-transition;

    .drawer--active & {
        transition: $fast-drawer-transition;
        @include transform(translateX(-$cart-mobile-width));

        @include media-query($medium-up) {
            @include transform(translateX(-$cart-desktop-width));
        }
    }
}

.drawer-cover {
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 0;
    background-color: $color-drawer-overlay;
    opacity: 0;
    pointer-events: none !important; // sass-lint:disable-line no-important
    z-index: $z-index-high;
    transition: opacity 0.25s ease-out;

    .drawer--active & {
        opacity: 0.3;
        height: 100%;
        pointer-events: auto !important; // sass-lint:disable-line no-important
    }
}

.ajax-cart__template {
    display: none;
}

.ajax-cart__discount-container--active {
    max-height: 100px;
    opacity: 1;
}


.featured-product__wrapper {
    @include display-flexbox();
    @include align-items(center);
    max-width: $width-site;
    padding: $section-desktop-space $gutter;
    margin: 0 auto;

    @include media-query($small) {
        padding: $section-mobile-space $gutter;
    }

    @include media-query($medium-down) {
        @include flex-direction(column);
    }
}

.featured-product__wrapper--full-width {
    max-width: none;
    padding: 0;

    @include media-query($small) {
        padding-bottom: $section-mobile-space;
    }

    @include media-query($medium) {
