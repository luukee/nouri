        background-color: $color-main-bg;
    }

    &.supports-no-js {
        background-color: $color-footer-bg;
    }
}

// 4021. Footer

.desktop-instagram-feed {
    display: none;

    @include media-query($medium-up) {
        display: block;
    }
}

.mobile-instagram-feed {

    @include media-query($medium-up) {
        display: none;
    }
}

.site-footer {
    background-color: $color-footer-bg;
    color: $color-footer-text;
    padding: ($gutter * 2) 0;
    @include border-top-section();
    margin-top: 4rem;

    @include media-query($medium-up) {
        margin-top: 8rem;
    }

    &>.page-width {
        max-width: $grid-xlarge;
        margin: 0 auto;
    }

    .site-footer--hidden & {
        display: none;
    }

    .supports-no-js & {
        display: block;
    }

    a {
        color: $color-footer-text;

        &:hover,
        &:focus {
            opacity: $link-hover-opacity;
        }
    }

    a.footer-logo-img {
        &:hover {
            opacity: 1;
        }
    }

    // top border
    .flex-footer {
        @include media-query($medium-up) {
            @include section-padding();
        }
    }

    .input-group--underline {
        border-color: $color-footer-text;

        & .btn {
            color: $color-footer-text;
        }
    }

    .input-group--underline input[type='email'] {
        padding-left: 0;
    }
    .icon {
        fill: $color-footer-text;
    }

    .footer-logo {
        margin: 2rem auto;

        @include media-query($medium-up) {
            text-align: left;
            margin: 0;
        }
    }
}

.site-footer--parallax {
    .site-footer {
        position: fixed;
        bottom: 0;
        left: 0;
        right: 0;
        z-index: $z-index-parallax-footer;
    }

    .content-for-layout {
        position: relative;
        z-index: $z-index-parallax-footer-body-content;
        margin-bottom: 400px;
    }
}

.site-footer__drawer-cover {
    display: none;
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    background-color: $color-main-bg;
}

.flex-footer {
    @include display-flexbox;
    @include align-items(flex-start);
    @include flex-wrap(wrap);
    width: auto;

    @include media-query($medium-up) {
        padding-bottom: $gutter * 2;
    }

}

.flex-footer__links {
    @include flex(1 1 100%);
    padding: 0 0 $gutter;

    @include media-query($small) {
        .flex-footer--full & {
            @include flex(1 0 100%);

            &:first-child {
                margin-top: $gutter;
            }
        }
    }

    @include media-query($medium-up) {
        display: flex;
        @include flex(1 0 auto);
        padding: 0 10px;

        .flex-footer--full & {
            @include flex(1 0 100%);
        }
    }

    .half {
        width: 50%;
    }
}

.flex-footer__item--one-quarter {
    @include media-query($medium-up) {
        flex: 0 0 25%;
    }
}

.flex-footer__item--one-third {
    @include media-query($medium-up) {
        flex: 0 0 33%;
    }
}

.flex-footer__item--half {
    @include media-query($medium-up) {
        flex: 0 0 50%;
    }
}

.flex-footer__item.newsletter {
    margin: 0 auto $gutter;
    max-width: 450px;

    @include media-query($medium) {
        display: flex;
        flex-direction: row;
        width: 100%;
        max-width: unset;
        margin: 0 0 1rem 0;
        justify-content: space-between;
    }

    @include media-query($large-up) {
        display: flex;
        flex: 0 0 50%;
        max-width: none;
    }
}

.site-footer__linklist {
    list-style: none;
    padding: 0;
    margin: 0;

    @include media-query($medium-up) {
        margin-top: 5px;
        text-align: left;

        .flex-footer--full & {
            text-align: center;
        }
    }
}

.site-footer__newsletter {
    display: flex;
    justify-content: center;
    @include media-query($medium) {
        flex: 0 1 70%;
    }
    @include media-query($large-up) {
        flex: 0 1 50%;
    }

    .input-group__btn {
        text-align: right;
        opacity: 1;
    }

    input[type="email"] {
        @include placeholder($color-footer-text, 1);
    }

    .btn--newsletter__submit {
        color: $color-footer-text;
    }

}

.site-footer__newsletter-label {
    display: block;
    font-size: rem(18);
    font-weight: $font-weight-bold;
    color: $color-footer-text;
    text-transform: uppercase;

    @include media-query($medium-up) {
        text-align: left;
    }
}

.site-footer__newsletter-subheading {
    font-size: rem(13);
    margin-bottom: rem(20);
    @include media-query($medium-up) {
        text-align: left;
    }
}

.site-footer__newsletter-message {
    padding-top: $gutter;

    & .form-success {
        color: $color-footer-text;
    }
}

.site-footer__linklist-item,
.social_icons_title {
    display: inline-block;
    padding: 0 ($gutter / 2);
    margin-bottom: $gutter / 2;
    font-size: rem(12);
    font-weight: $font-weight-bold;
    letter-spacing: normal;

    @include media-query($medium-up) {
        display: block;
        padding: 0 20px 0 0;

        .flex-footer--full & {
            display: inline-block;
            padding: 5px 10px;
        }
    }
}

// 4231. Footer Bottom
.site-footer__bottom {
    @include display-flexbox;
    @include align-items(flex-start);
    @include justify-content(space-between);
    @include flex-wrap(wrap);
    @include flex-direction(column);

    @include media-query($medium-up) {
        @include flex-direction(row);
        padding: 4rem 0 0 0;
        border-top: 1px solid black;
    }

    // copyright section
    .site-footer__copyright {
        list-style-type: none;
        display: flex;
        flex-direction: column;
        padding: 0;

