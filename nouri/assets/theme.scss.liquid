/*================ Site Header ================*/

.site-header {
  position: relative;
  transition: transform 0.25s ease-out;
  z-index: $z-index-header;
}

.site-header--transparent,
.site-header--homepage {
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  z-index: $z-index-header;

  html.supports-no-js & {
    position: relative;
  }
}

.site-header--fixed {
  position: fixed;
  top: 0;
  width: 100%;
  @include prefix(animation, slideDown 0.25s ease-out, webkit spec);
}

.site-header__wrapper {
  position: relative;
  width: 100%;
  background-color: $color-header;
  z-index: $z-index-header;
  transition: background-color 0.25s ease-out;
  -webkit-transform:translate3d(0,0,0);
  .site-header--transparent & {
    background-color: $site-header-wrapper-color;
  }

  .navigation-open &,
  html.supports-no-js & {
    background-color: $color-header;
  }

  .site-header--fixed & {
    background-color: $color-header;
  }

  svg {
    fill: $color-header-links;
  }
}

.site-header__wrapper--transitioning {
  transition: background-color 0.25s ease-out, margin-top 0.25s ease-out;
}

.announcement-bar {
  display: none;
  text-align: center;
  position: relative;
  text-decoration: none;
  z-index: $z-index-header;
  .announcement-bar-image {
    max-width: 100px;
    margin-bottom: -5px;
  }
}

.announcement-bar--visible {
  display: block;
}

.announcement-bar__message {
  font-size: em(14);
  font-weight: $font-weight-header;
  padding: 8px $gutter;
  margin-bottom: 0;

  @include media-query($small) {
    padding: 8px calc(#{$gutter} + 30px);
  }

  .icon {
    fill: currentColor;
  }
}

.announcement-bar__close {
  position: absolute;
  right: $gutter;
  padding: 0;
  width: 30px;
  height: 30px;
  line-height: 100%;
  top: 50%;
  @include transform(translateY(-50%));

  @include media-query($small) {
    right: 10px;
  }

  .icon {
    width: 12px;
    height: 12px;
    fill: currentColor;
  }
}


