/*================ AJAX Cart Footer ================*/

.cart-drawer__footer {
    min-height: 230px;
    padding: 0 $gutter;
    border-top: 1px solid darken($color-secondary-bg, 5%);
    background-color: $settings-color-main-bg;
    text-align: center;
    opacity: 1;
    transition: opacity 0.25s ease-out, min-height 0.25s ease-out;

    .cart-drawer--empty & {
        opacity: 0;
        pointer-events: none;
        min-height: 0;
        max-height: 0;
    }

    @include media-query($medium-up) {
        padding: 0 ($gutter * 2);
    }
}

.cart-drawer__footer-container {
