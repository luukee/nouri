/*================ AJAX Cart ================*/

.cart-drawer {
    @include display-flexbox();
    @include flex-flow(column);
    width: 100%;
    height: 100%;
    background-color: $settings-color-header;
}

.cart-drawer__content {
    display: block;
    @include flex(1 100);
    overflow: auto;
    -webkit-overflow-scrolling: touch; // sass-lint:disable-line no-misspelled-properties
    @include prefix(transition, all 0.25s ease-out, webkit spec);

    .cart-drawer--checkout-select-active & {
        @include flex(0 100);
    }
}

.cart-drawer__content-container {
    overflow: hidden;
    padding: (2 * $gutter) 0;

    @include media-query($small) {
        padding: $gutter 0;
    }
}

.cart-drawer__empty-text {
    opacity: 0;
    height: 0;
    pointer-events: none;
    transition: opacity 0.25s ease-out;

    .cart-drawer--empty & {
        height: auto;
        opacity: 1;
        pointer-events: auto;
        margin-top: $gutter * 3;
    }

    .cart-drawer--no-cookies & {
        display: none;
    }
}

.cart-drawer__no-cookies {
    display: none;

    .cart-drawer--no-cookies & {
        display: block;
        margin-top: $gutter * 3;
        padding: 0 ($gutter * 2);

        @include media-query($small) {
            padding: 0 $gutter;
        }
    }
}

/*================ AJAX Cart Header ================*/

.cart-drawer__header {
    position: relative;
    height: 60px;
    padding: 0 $gutter;
    box-sizing: content-box;
    border-bottom: 1px solid darken($color-secondary-bg, 5%);

    @include media-query($medium-up) {
        padding: 0 ($gutter * 2);
    }
}

.cart-drawer__header-container {
    position: relative;
    width: 100%;
    height: 100%;
}

.cart-drawer__header-title {
    position: absolute;
    top: 50%;
    left: 50%;
    min-width: 215px;
    @include transform(translate(-50%, -50%));
}

.cart-drawer__close-button {
    position: absolute;
    top: 50%;
    right: -16px;
    @include transform(translateY(-50%));

    .icon {
        width: 14px;
        fill: $color-primary;

        @include media-query($medium-up) {
            width: 18px;
        }
    }
}

/*================ AJAX Cart Items ================*/

.cart-drawer__item-list {
    transition: opacity 0.25s ease-out;

    .cart-drawer--empty &,
    .cart--no-cookies & {
        display: none;
    }

    @include media-query($medium-up) {
        padding: 0 ($gutter * 2);
    }

    @include media-query($small) {
        padding: 0 $gutter;
    }
}

.cart-drawer__item {
    position: relative;
    font-size: rem(12);
    padding-bottom: $gutter * 2;
    opacity: 0;
    transition: 0.25s all ease-out;
    @include transform(translateX(20%));

    &.ajax-cart__item--remove {
        @include animation(remove 0.25s ease-out);
        @include transform(translateX(50%));
        max-height: 0;
        overflow: hidden;
        opacity: 0;
        transition: none;
    }
}

@for $i from 1 through 10 {
    .cart-drawer__item:nth-of-type(#{$i}) {
        transition-delay: #{0.2 + (0.08 * $i)}s;
        -webkit-transition-delay: #{0.2 + (0.08 * $i)}s;

        .drawer--active & {
            opacity: 1;
            @include transform(translateX(0%));
        }
    }
}

@include keyframes(remove) {
    0% {
        @include transform(translateX(0%));
        max-height: 250px;
        opacity: 1;
    }

    33% {
        @include transform(translateX(50%));
        max-height: 250px;
        opacity: 0;
    }

    100% {
        @include transform(translateX(50%));
        max-height: 0;
        opacity: 0;
    }
}

.cart-drawer__item-image {
    display: block;
    position: absolute;
    top: 0;
    left: 0;
    width: 15%;
    padding-top: 15%;
    border-radius: 50%;
    background-size: cover;
    background-position: center center;
    background-color: $color-input-background;

    @include media-query($medium-up) {
        width: 20%;
        padding-top: 20%;
    }
}

.cart-drawer__item-content {
    margin-left: 20%;
    padding-left: $gutter;
    padding-right: 70px;
    padding-bottom: $gutter;

    @include media-query($small) {
        margin-left: 15%;
        padding-bottom: $gutter;
    }
}

.cart-drawer__item-title {
    margin-bottom: 0;
    display: inline-block;
    font-size: rem(18);
    text-decoration: none;

    @include media-query($small) {
        font-size: rem(16);
    }
}

.cart-drawer__item-subtitle {
    margin-bottom: 0;
