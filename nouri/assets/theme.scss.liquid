    background-color: $color-header;

    @include media-query($medium-up) {
        margin-bottom: $gutter * 2.5;
    }
}

.giftcard-header__shop-name {
    margin: 0;
}

.giftcard__qr-code {
    margin: ($gutter * 2) 0;

    img {
        margin: 0 auto;
    }
}

.giftcard__apple-wallet-image {
    display: block;
    margin: 0 auto;
}

.giftcard__content {
    max-width: 600px;
    margin: 0 auto;
    padding: 0 $gutter;
}

.giftcard__wrap {
    position: relative;
