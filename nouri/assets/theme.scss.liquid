$container-padding-bottom: 35px;
$transition-buttons: all 0.35s cubic-bezier(0, 0, 0, 1);
$transition-slideshow: all 0.5s cubic-bezier(0, 0, 0, 1);
$transition-image-slideshow: opacity 0.8s cubic-bezier(0.44, 0.13, 0.48, 0.87),
filter 400ms, -webkit-filter 400ms;
$button-size: 30px;

.slideshow {
    @include display-flexbox;
    position: relative;
    overflow: hidden;
    max-height: calc(100vh - #{$container-padding-bottom});

    @include media-query($small) {
        margin-top: 60px;
    }

    @include media-query($medium-up) {
        margin-top: 76px;
    }
}

.slideshow--adapt {
    @include media-query($medium-up) {
        margin-top: 60px;
    }
}

.slideshow--full .slideshow {
    height: calc(100vh - #{$container-padding-bottom});
}

.slideshow--adapt .slideshow {

    @include media-query($medium-up) {
        overflow: visible;
        width: 100%;
        margin: 0 auto;
    }
}

.slideshow__overlay {
    @include overlay(3);
}

.slideshow__slide {
    position: absolute;
    height: 100%;
    width: 100%;
    opacity: 0;
    z-index: 0;
    transition: $transition-slideshow;
    background-repeat: no-repeat;
    background-size: 177%;
    background-position: center top;
    @include media-query($medium-up) {
        background-size: 100% auto;
        background-position: bottom;
    }
    .slideshow-row,
    .slideshow-row__image-wrapper {
        height: 100%;
        @include media-query($medium-up) {
            display: flex;
            align-items: center;  
        }
    }
    @include media-query($large-up) {
        .slideshow-row {
            height: 100%;
            display: flex;
            align-items: center;
        }
        .slideshow-row__item {
            height: 100%;
        }
        .slideshow-row__text {
            padding: 140px 40px;
            margin: 0 auto;
        }
    }
}

.slideshow__slide--active {
    transition: $transition-slideshow;
    opacity: 1;
    z-index: $z-index-base + 2;
}

.slideshow__slide--transitioning {
    opacity: 1;
    z-index: $z-index-base + 1;
}

.slideshow__image {
    height: 100%;
    width: 100%;
    object-fit: cover;
    z-index: $z-index-base + 1;
    transition: $transition-image-slideshow;
    font-family: "object-fit: cover";

    @include media-query($medium-up) {
        &.mobile {
            display: none;
        }
    }

    @include media-query($small) {
        &.desktop {
            display: none;
        }
    }

    .placeholder-background {
        background-color: $color-main-bg;
    }

    .placeholder-svg {
        position: absolute;
        top: 50%;
        left: 50%;
        width: auto;
        max-height: none;
        max-width: none;
        @include transform(translate(-50%, -50%));
    }
}

.slideshow__video,
.slideshow__video iframe {
    opacity: 0;
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 101%;
    visibility: hidden;
    transition: $transition-slideshow;
    pointer-events: none;

    .slideshow--adapt & {
        // min-height: 56.25vw;
    }

    @include media-query($small) {
        display: none;
    }

    // https://developer.mozilla.org/en-US/docs/Web/CSS/Media_Queries/Using_media_queries#aspect-ratio
    @media (min-aspect-ratio: 16/9) {
        height: 300%;
        top: -100%;
    }

    @media (max-aspect-ratio: 16/9) {
        width: 300%;
        left: -100%;
    }

    .slideshow__video--loaded:not(.slideshow__video--paused) & {
        opacity: 1;
        visibility: visible;
    }
}

.slideshow__video::after {
    content: "";
    display: block;
    position: absolute;
    top: 0;
    right: 0;
    bottom: 0;
    left: 0;
    z-index: $z-index-base;
}

.slideshow__play-button {
    opacity: 0;
    position: relative;
    margin: 0 auto;
    height: 50px;
    width: 50px;
    padding: 0;
    transition: transform 0.5s ease;

    &:hover {
        @include transform(scale(1.1));
    }

    &:hover,
    &:disabled {
        opacity: 0;
    }

    .slideshow__video--loaded & {
        opacity: 1;
    }

    @include media-query($small) {
        display: none;
    }
}

.slideshow__play-button-icon {
    position: absolute;
    top: 50%;
    left: 50%;
    width: 8px;
    height: 8px;
    border-color: transparent transparent transparent $settings-color-header;
    border-width: 0 0 0 7px;
    border-style: double;
    transition: border 0.1s ease;
    @include transform(translate(-42%, -50%));

    .is-paused & {
        width: 0;
        height: 0;
        border-top: 4px solid transparent;
        border-left: 8px solid $settings-color-header;
        border-bottom: 4px solid transparent;
    }
}

.slideshow__play-button-circle {
    position: absolute;
    top: 50%;
    left: 50%;
    background-color: transparent;
    border: 2px solid $settings-color-header;
    height: $button-size;
    width: $button-size;
    -moz-border-radius: $button-size / 2;
    -webkit-border-radius: $button-size / 2;
    border-radius: $button-size / 2;
    @include transform(translate(-50%, -50%));
}

.slideshow__text-container {
    @include media-query($small) {
            text-align: center;
    position: absolute;
    left: 0;
    @include transform(translate3d(0, -50%, 0));
    }
    z-index: $z-index-base + 2;
    @include media-query($medium-down) {
        width: 100%;
        max-width: 100%;
    }
    @include media-query($large-up) {
        top: 70%;
    }
}

.slideshow__text-content {
    opacity: 0;

    &.has-animated {
        @include prefix(animation, fadeUp 0.5s ease-out 0.5s 1 forwards, webkit spec);
    }

    .supports-no-js &,
    .animations--disabled & {
        opacity: 1;
    }

    .slideshow--adapt:not(.slideshow--center-cta) & {
        @include media-query($small) {
