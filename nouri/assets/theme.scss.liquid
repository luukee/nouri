// 10564. collection template
.collection-template:not(.featured-collection) {
    .card__wrapper {
        display: flex;
        height: auto !important;
        flex-direction: column;
        @include media-query($small) {
            overflow: visible;
        }
        @include media-query($medium-up) {
            flex-direction: row;
            align-items: center;
        }
    }
    .image-container {
        height: auto;
        @include media-query($medium-up) {
            width: 50%;
        }
    }
    .card__image-container {
        @include media-query($medium-up) {
            width: 50%;
        }
    }
    .card__image--align {
        object-fit: contain;
    }
    .card__name {
        font-size: 2.5rem;
        font-weight: 100;
        @include media-query($medium-up) {
                max-width: min-content;
        }
    }

    .card__info {
        @include media-query($small) {
            align-items: center;
            display: flex;
            flex-direction: column;
            p {
                text-align: center;
            }
        }
        @include media-query($medium) {
            padding: 0 2.8rem;
            background: transparent;
            width: 50%;
            bottom: unset;
        }
        @include media-query($large-up) {
            padding: 0 8.8rem;
            background: transparent;
            width: 50%;
            bottom: unset;
        }
    }
    .card:nth-child(even) {
        .card__wrapper {
            @include media-query($medium-up) {
                flex-direction: row-reverse;
            }
        }
       
    } // END even products
    .card:nth-child(odd) {
        .card__info.card__info--aligned {
            @include media-query($medium) {
                right: 0;
                left: unset;
                padding-right: 2.8rem
            }
            @include media-query($large-up) {
                right: 0;
                left: unset;
                padding-right: 8.8rem
            }
        }
    } // END odd products
}

