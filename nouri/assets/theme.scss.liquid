}

/*================ Navigation ================*/
.navigation {
    position: absolute;
    top: 0;
    left: 0;
    background-color: $color-main-bg;
    max-height: 0;
    width: 100%;
    text-align: center;
    overflow-x: hidden;
    overflow-y: auto;
    -webkit-overflow-scrolling: touch; // sass-lint:disable-line no-misspelled-properties
    opacity: 0;
    transition: opacity 0.25s ease-out;
    pointer-events: none;
    z-index: $z-index-base;

    html.supports-no-js & {
        max-height: none;
        position: relative;
        opacity: 1;
        pointer-events: auto;
    }
}

.navigation__container {
    width: 100%;
    min-height: 100%;
    padding-top: ($gutter * 3);
    padding-bottom: ($gutter * 3);
}

.navigation-open {
    .navigation {
        opacity: 1;
        pointer-events: auto;
        box-shadow: 0 10px 28px -15px #0000002b;
    }
}

.navigation--is-transitioning {
    .navigation {
        position: absolute;
        max-height: none;
        height: 100vh;
        width: 100%;
    }

    .content-for-layout {
        display: block !important; // sass-lint:disable-line no-important
    }

    .site-footer {
        display: block !important; // sass-lint:disable-line no-important
    }
}

.navigation__entrance-animation {
    max-height: 1000px;
    opacity: 0;
    @include transform(translateY(20px));
    transition: max-height 0.01s, transform 0.25s ease-out, opacity 0.25s ease-out;

    &.navigation__entrance-animation--active,
    .navigation-open--editor & {
        max-height: 1001px;
        opacity: 1;
        @include transform(translateY(0));
    }

    html.supports-no-js & {
        opacity: 1;
    }
}

nav.site-header__section.site-header__section--button {
    @include media-query($large-up) {
        // display: none;
    }
}

/*================ Navigation Links ================*/
.navigation__links {
    list-style: none;
    padding-left: 0;
    margin: 0 0 $gutter;

    @include media-query($large-up) {
        display: flex;
        align-items: center;
        list-style: none;
        padding: 0;
    }

}
// desktop navigation links
.navigation__desktop,
.navigation__desktop--right {
    .navigation__links {
        display: inline-flex;
        margin: 0;
    }
}

.navigation__link {
    &.button {
        .shop-btn {
            display: inline-block;
            text-decoration: none;
            text-transform: lowercase;
            width: auto;
            border-radius: 50px;
            border: 1px solid $settings-color-primary;
            vertical-align: middle;
            cursor: pointer;
            transition: all 0.1s linear;
            box-sizing: border-box;
            margin-bottom: 3px;
            padding-left: 3.5rem;
            padding-right: 3.5rem;
            color: $settings-color-primary;
            background-color: transparent;
            font-size: 1.2rem;
            line-height: 2.1;
            font-weight: 700;
            opacity: 1;
            transition: box-shadow 0.25s ease-out;

            &:hover {
                box-shadow: 0 0 0px 2px $settings-color-primary;
            }
        }
        .btn--secondary {
            color: $color-primary;
        }
    }
    &.button.navigation__link--active .shop-btn {
        background-color: $settings-color-primary;
        color: $settings-color-header;
    }
    a:not(.shop-btn) {
        display: inline-block;
        position: relative;
        font-size: rem(14);
        line-height: line-height(64, 18);
        font-weight: $font-weight-bold;
        letter-spacing: normal;
        color: $color-heading-text;
        padding: 0 ($gutter / 2);
        text-decoration: none;
        opacity: 1;

        @include media-query($medium-up) {
            font-size: rem(12);
            line-height: line-height(42, 20);
            font-weight: $font-weight-bold;
        }
    }
}

.navigation__link > a:not(.shop-btn),
.navigation__link.navigation__has-sublinks--collapsed > button {
    transition: transform 0.25s ease-out;

    &:after {
        content: '';
        display: block;
        position: relative;
        bottom: rem(9);
        width: 0;
        margin: 0 auto;
        height: 2px;
        background-color: $color-body-text;
        transition: all 0.25s;
    }

    @include media-query($medium-up) {
        &:hover {
            @include transform(translateY(-5px));

            &:after {
                @include transform(translateY(5px));
                width: 100%;
            }
        }

        &:hover:active {
            @include transform(translateY(0));

            &:after {
                @include transform(translateY(0));
            }
        }
    }
}

.navigation__expand-sublinks {
    @include prefix('user-select', 'none', moz ms webkit spec);
    @include prefix('appearance', none, webkit moz spec);
    position: relative;
    padding: 0 ($gutter / 2);
    font-size: rem(14);
    line-height: line-height(64, 18);
    font-weight: $font-weight-bold;
    letter-spacing: normal;
    color: $color-heading-text;
    background-color: transparent;
    border: 0;
    text-decoration: none;
    opacity: 1;
    transition: all 0.25s ease-out;

    &:after {
        content: '';
        display: block;
        position: relative;
        bottom: rem(9);
        width: 0;
        margin: 0 auto;
        height: 2px;
        background-color: $color-body-text;
        transition: all 0.25s;
    }

    @include media-query($medium-up) {
        font-size: rem(20);
        line-height: line-height(48, 20);
        font-weight: $font-weight-bold;

        &:hover {
            @include transform(translateY(-5px));

            &:after {
                @include transform(translateY(5px));
            }
        }

        &:hover:active {
            @include transform(translateY(0));

            &:after {
                @include transform(translateY(0));
            }
        }
    }



    svg {
        width: 10px;
        position: absolute;
        right: -$gutter / 2;
        top: 50%;
        margin-top: -10px;
        fill: $color-primary;
        @include transform(rotateZ(180deg));
        transition: transform 0.25s ease-out;

        .navigation__has-sublinks--collapsed & {
            @include transform(rotateX(0deg));
        }
    }
}

.navigation__expand-sublinks--third-level {
    font-weight: $font-weight-body-custom;
    line-height: line-height(42, 14);
    text-transform: none;

    @include media-query($medium-up) {
        font-size: rem(16);
        line-height: line-height(42, 16);

        &:hover {
            opacity: $link-hover-opacity;
            @include transform(translateY(0));

            &:after {
                @include transform(translateY(0));
            }
        }
    }
}

.navigation__sublinks-container {
    transition: all 0.15s ease-out;
    background-color: $settings-color-main-bg;
    overflow: hidden;
    visibility: visible;

    @include media-query($medium-down) {
        .navigation__has-sublinks--collapsed & {
            max-height: 0 !important; // sass-lint:disable-line no-important
            visibility: hidden;
        }
    }

    html.supports-no-js & {
        max-height: none !important; // sass-lint:disable-line no-important
        visibility: visible;
    }
}

.navigation__sublinks {
    overflow: hidden;
    list-style: none;
    padding: $gutter 0;
}

.navigation__sublinks--third-level {
    overflow: hidden;
    list-style: none;
    padding: 0 0 ($gutter / 2);

    @include media-query($medium-up) {
        margin-top: -5px;
