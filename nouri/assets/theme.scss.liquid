    }

    .hero--medium & {
        padding: ($gutter * 4) 0;
    }

    .hero--large & {
        padding: ($gutter * 6) 0;
    }

    .hero--x-large & {
        padding: ($gutter * 8) 0;
    }

    @include media-query($medium-up) {
        .hero--x-small & {
            padding: $gutter 0;
        }

        .hero--small & {
            padding: ($gutter * 3) 0;
        }

        .hero--medium & {
            padding: ($gutter * 7) 0;
        }

        .hero--large & {
            padding: ($gutter * 11) 0;
        }

        .hero--x-large & {
